    "import numpy as np\n",
    "\n",
    "X = np.random.binomial(1, .4, 1000)\n",
    "Y = np.random.binomial(1, .4, 1000)\n",
    "\n",
    "c = np.zeros((2, 2))\n",
    "for i in [0,1]:\n",
    "    for j in [0,1]:\n",
    "        for n in range(1000):\n",
    "            if X[n] == i and Y[n] == j:\n",
    "                c[i,j] += 1\n",
    "                \n",
    "np.sum(c)"
   ]
